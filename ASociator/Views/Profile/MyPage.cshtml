@model ASociator.ViewModels.UserProfileViewModel

@{
    ViewData["Title"] = Model.FirstName + " " + Model.LastName;
}

@await Html.PartialAsync("_SelectLanguagePartial")

<h2>
    @ViewData["Title"] |
    <a asp-action="Edit" class="btn btn-warning" asp-route-id="@Model.Id">@ViewData["Edit"]</a>
</h2>

@await Html.PartialAsync("UserProfilePartial", Model)
